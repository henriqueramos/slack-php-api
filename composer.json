{
    "name": "jolicode/slack-php-api",
    "description": "An up to date PHP client for Slack's API",
    "keywords": ["slack", "slackapi", "api", "client", "openapi", "swagger", "sdk"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lo√Øck Piera",
            "email": "pyrech@gmail.com"
        },
        {
            "name": "JoliCode",
            "email": "coucou@jolicode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoliCode\\Slack\\Api\\": "generated/",
            "JoliCode\\Slack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoliCode\\Slack\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0.2",
        "jane-php/open-api-runtime": "~7.1",
        "psr/http-client-implementation": "*",
        "php-http/client-common": "^1.9 || ^2.0",
        "php-http/discovery": "^1.11",
        "php-http/multipart-stream-builder": "^1.1",
        "symfony/options-resolver": "6.0.19",
        "symfony/serializer": "6.0.19",
        "symfony/yaml": "6.0.19",
        "league/uri": "6.7.2"
    },
    "require-dev": {
        "jane-php/open-api-2": "~7.1",
        "symfony/http-client": "^5.4 || ^6.0",
        "nyholm/psr7": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.2.2",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/process": "^5.4 || ^6.0",
        "opis/json-schema": "^1.0"
    },
    "conflict": {
        "php-http/httplug": "< 2.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
